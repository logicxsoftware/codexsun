{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Database": {
    "Provider": "MariaDb",
    "MariaDbServerVersion": "11.5.2",
    "MinPoolSize": 5,
    "MaxPoolSize": 200,
    "Master": {
      "ConnectionString": "server=localhost;port=3306;database=codexsun_dev;user=root;password=Computer.1;"
    },
    "Tenant": {
      "ConnectionStringTemplate": "server=localhost;port=3306;database={database};user=root;password=Computer.1;"
    },
    "Migration": {
      "TenantBatchSize": 25,
      "MaxParallelTenants": 4
    }
  },
  "MultiTenancy": {
    "HeaderName": "X-Tenant-Id",
    "Cache": {
      "MetadataTtlSeconds": 120,
      "FeatureTtlSeconds": 60
    },
    "DefaultTenant": {
      "Identifier": "default",
      "Name": "Default Tenant",
      "DatabaseName": "tenant_dev",
      "FeatureSettingsJson": "{}",
      "IsolationMetadataJson": "{}"
    }
  },
  "Authentication": {
    "Jwt": {
      "Issuer": "",
      "Audience": "",
      "SigningKey": ""
    }
  }
}
