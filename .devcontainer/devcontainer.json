{
  "name": "aspire-app",
  "dockerComposeFile": [
    "docker-compose.yml"
  ],
  "service": "apphost",
  "workspaceFolder": "/src",
  "remoteUser": "root",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-dotnettools.csharp",
        "ms-azuretools.vscode-docker",
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode"
      ]
    }
  },
  "forwardPorts": [
    7040,
    7041,
    7042,
    7043,
    7045,
    7046,
    7047
  ],
  "portsAttributes": {
    "7040": {
      "label": "aspire dashboard"
    },
    "7041": {
      "label": "cxserver http"
    },
    "7042": {
      "label": "cxserver https"
    },
    "7043": {
      "label": "frontend vite"
    },
    "7045": {
      "label": "aspire otlp"
    },
    "7046": {
      "label": "aspire mcp"
    },
    "7047": {
      "label": "aspire resource service"
    }
  },
  "postCreateCommand": "dotnet restore codexsun.sln && cd cxweb && npm install",
  "postStartCommand": "dotnet --info && node --version && npm --version"
}
