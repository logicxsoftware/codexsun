{
  "name": "aspire-app",
  "build": {
    "dockerfile": "Dockerfile",
    "context": ".."
  },
  "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
  "remoteUser": "root",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-dotnettools.csharp",
        "ms-azuretools.vscode-docker",
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode"
      ]
    }
  },
  "forwardPorts": [
    5000,
    5001,
    5173,
    18888
  ],
  "portsAttributes": {
    "5000": {
      "label": "aspire server http"
    },
    "5001": {
      "label": "aspire server https"
    },
    "5173": {
      "label": "vite dev server"
    },
    "18888": {
      "label": "aspire dashboard"
    }
  },
  "postCreateCommand": "dotnet restore codexsun.sln && cd cxweb && npm install",
  "postStartCommand": "dotnet --info && node --version && npm --version"
}
